class Node:
	def __init__(self,v):
		self.val=v
		self.next=None

class LL:
	def __init__(self):
		self.head=None
	def insert(self,v):
		node=Node(v)
		current=self.head
		if current==None:
			self.head=node
		else:
			while current.next!=None:
				current=current.next
			current.next=node
	def printLL(self):
		current=self.head
		while current!=None:
			print(current.val,end=" ")
			current=current.next
		print()

def reverseLL(l):
	curr=l.head
	if curr==None:
		return
	prev=None
	nxt=None
	while curr!=None:
		nxt=curr.next
		curr.next=prev
		prev=curr
		curr=nxt
	l.head=prev
	l.printLL()

l=LL()
l.insert(2)
l.insert(4)
l.insert(6)
l.insert(8)
l.insert(10)
reverseLL(l)
