class Node:
	def __init__(self,v):
		self.val=v
		self.next=None

class LL:
	def __init__(self):
		self.head=None
	def insert(self,v):
		node=Node(v)
		current=self.head
		if current==None:
			self.head=node
		else:
			while current.next!=None:
				current=current.next
			current.next=node
	def printLL(self):
		current=self.head
		while current!=None:
			print(current.val,end=" ")
			current=current.next
		print()

def reverse(head,k):
	curr=head
	next=None
	prev=None
	c=0
	while curr is not None and c<k:
		next=curr.next
		curr.next=prev
		prev=curr
		curr=next
		c+=1
	if next is not None:
		head.next=reverse(next,k)
	return prev

l=LL()
l.insert(10)
l.insert(20)
l.insert(30)
l.insert(40)
l.insert(50)
l.insert(60)
l.head=reverse(l.head,3)
l.printLL()
