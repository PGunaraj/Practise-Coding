def stringCompression(string):
	index=0
	l=0
	r=1
	count=1
	if len(string)<2:
		return "error"
	while r<len(string):
		while r<len(string) and string[l]==string[r]:
			r+=1
			count+=1
		if index<len(string)-1:
			string[index]=string[l]
			index+=1
			string[index]=str(count)
			index+=1
			l=r
			r=r+1
			count=1
		else:
			return "error"
	if l==len(string)-1:
		if index<len(string)-1:
			string[index]=string[l]
			index+=1
			string[index]=str(count)
		else:
			return "error"
	string=''.join(string)
	return string
	
s="aaabcccd"
s=list(s)
print(stringCompression(s))
